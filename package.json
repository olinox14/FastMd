{
	"name": "fullmd",
	"publisher": "olinox14",
	"displayName": "FullMd",
	"description": "Fast Markdown formatting for Visual Studio Code",
	"version": "0.1.1",
    "homepage": "https://github.com/olinox14/FullMd",
    "repository": {
        "type": "git",
        "url": "https://github.com/olinox14/FullMd.git"
    },
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"markdown",
		"formatting",
		"shortcuts"
	],
	"activationEvents": [
		"onLanguage:markdown"
	],
	"main": "./fullmd.js",
