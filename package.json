		"keybindings": [
			{
				"command": "fullmd.addLinebreak",
				"key": "alt+enter",
				"when": "editorTextFocus && editorLangId == 'markdown'"
			},
			{
				"command": "fullmd.escape",
				"key": "ctrl+/",
				"when": "editorTextFocus && editorLangId == 'markdown'"
			},
			{
				"command": "fullmd.headerUp",
				"key": "ctrl+d",
				"when": "editorTextFocus && editorLangId == 'markdown'"
			},
			{
				"command": "fullmd.headerDown",
				"key": "ctrl+shift+d",
				"when": "editorTextFocus && editorLangId == 'markdown'"
			},
			{
				"command": "fullmd.setH0",
				"key": "ctrl+shift+9",
				"when": "editorTextFocus && editorLangId == 'markdown'"
			},
			{
				"command": "fullmd.setH1",
				"key": "ctrl+shift+1",
